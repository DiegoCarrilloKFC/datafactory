{
	"name": "pl_dwh_Planta_Ecu_Compras",
	"properties": {
		"description": "Carga Informaci√≥n Planta Ecuador",
		"activities": [
			{
				"name": "plt_FacPedidoCompra",
				"type": "SqlServerStoredProcedure",
				"dependsOn": [
					{
						"activity": "FechaCarga",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
